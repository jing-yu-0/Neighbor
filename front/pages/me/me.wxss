.iconfont {
  font-family: "iconfont" !important;
  font-size: 33rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

button::after {
  outline: none;
  border: none;
}

button {
  display: inline;
  line-height: 50rpx;
  background-color: white;
  padding: 0;
  margin-left: 10rpx;
  font-size: 33rpx;
  font-weight: 200;
}

.icon-about:before {
  content: "\e614";
}

.icon-xiaoxi:before {
  content: "\e7f3";
}

.icon-yueding:before {
  content: "\e6d1";
}

.icon-shoucang:before {
  content: "\e633";
}

.icon-wocanyude:before {
  content: "\e7d5";
}

.icon-lianxi:before {
  content: "\e649";
}

.icon-follow:before {
  content: "\e703";
}

.icon-wodefabu:before {
  content: "\e668";
}

.icon-kefu:before {
  content: "\e618";
}

.top {
  background-color: white;
}

.user_message {
  display: flex;
  flex-direction: row;
  padding-left: 20rpx;
  padding-top: 50rpx;
}

.user_image image {
  width: 130rpx;
  border-radius: 50%;
}

.user_detail {
  margin-left: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 10rpx;
}

.user_nickname {
  font-size: 35rpx;
  font-weight: 800;
  color: cadetblue;
}

.user_time {
  font-weight: 200;
  color: #999;
  font-size: 25rpx;
}

.me_operation {
  margin-top: 20rpx;
}

.me_operation view {
  height: 50rpx;
  line-height: 50rpx;
  background-color: white;
  margin-top: 2rpx;
  padding: 20rpx;
  font-size: 33rpx;
  font-weight: 200;
  letter-spacing: 10rpx;
}

.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.5);
  display: none;
}

.submit {
  display: block;
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
  height: 80rpx;
  font-weight: 800;
  line-height: 80rpx;
}

.zan-dialog__container {
  position: fixed;
  bottom: 500rpx;
  width: 80%;
  margin-left: 10%;
  height: 250rpx;
  background: #f8f8f8;
  transform: translateY(300%);
  transition: all 0.4s ease;
  z-index: 12;
  border-radius: 20rpx;
}

.tips {
  margin: 10rpx;
  text-align: center;
  font-weight: 800;
  color: #999;
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}
 .pulse-bubble-3 {
  animation: pulse 0.4s ease 0.4s infinite alternate;
}

.sun {
  width: 35px;
  height: 35px;
  border-radius: 50%;
  background-color: #ffab91;
}
