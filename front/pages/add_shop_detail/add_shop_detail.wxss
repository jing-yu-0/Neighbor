.tips{
    background-color: wheat;
    padding: 20rpx;
    margin: 10rpx;
    color: #999;
    font-size: 25rpx;
}
.shop-main{
    width: 100%;
    margin: 10px;
    height:100px;
    display: flex;
    flex-direction: row;
}
.shop-title{
    width:50%;
    /* margin: 10px; */
    margin-right: 10px;
    display: block;
}
.shop-title textarea{
    max-width: 100%;
    resize:none;
}

.shop-logo{
    width: 40%;
    height: 90%;
    margin-right: 20px;
    border: 5px rgb(56, 58, 58);
}

.shop-logo image{
    max-width: 100%;
    max-height: 100%;
}
.shop-item{
    display: flex;
    flex-direction: row;
    margin: 5px;
    margin-bottom: 20px;
    border-bottom: 2rpx dashed #ccc;
    font-size: 30rpx;
}
.put-left{
 width: 30%;
 color: rgb(39, 42, 44);
}
.put-right{
 /* width: 60%; */
 color: rgb(59, 60, 61)
}
.shop_pic{
    border-top: 5rpx dashed #ccc;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: 5px;
  }
.shop_pic image{
    width:218rpx;
    height: 145rpx;
    margin-top: 10rpx;
  }
.submit {
    position: fixed;
    bottom: 0;
    width: 100%;
}
.main {
    text-align: center;
    font-size: 40rpx;
    padding: 20rpx;
    font-weight: 1000;
    color: white;
    background-color: #50D2C2;
}

.curloc{
    font-size: 27rpx;
    color: #999;
    float: left;
    text-align: left;
    margin-left: 0;
    /* background: rgb(255, 255, 255); */
}