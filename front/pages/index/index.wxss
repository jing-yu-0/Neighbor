.toploc{
  position: relative; 
  height: 5%; 
  margin-bottom: 3px;
}
.curxq{
  margin-top: 5px;
  background-color: rgb(241, 239, 236);
  color: #535353; 
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  font-weight: 700;
  height: 80%;
  float:left;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-iconfontpinglun:before {
  content: "\e62f";
}

.icon-dianzan:before {
  content: "\e61c";
}

.icon-zhuanfa:before {
  content: "\e627";
}

.imageContainer {
  width: 100%;
  height: 320rpx;
}

.item {
  height: 400rpx;
}

.itemImg {
  position: absolute;
  width: 100%;
  height: 240rpx;
  z-index: 5;
  opacity: 1;
  top: 5%;
}

.imageContainer .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 40rpx;
}

.imageContainer .wx-swiper-dot {
  width: 28rpx;
  display: inline-flex;
  height: 8rpx;
  margin-left: 10rpx;
  justify-content: space-between;
}

.active {
  opacity: 1;
  z-index: 10;
  height: 290rpx;
  top: 0;
  transition: all 0.2s ease-in 0s;
  box-shadow: 0px 0px 20rpx #000;
}

.category {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}

.category_item {
  display: flex;
  flex-direction: column;
  text-align: center;
  width: 15%;
  margin: 2%;
  margin-top: 0;
}

.category_item image {
  background-color: white;
  width: 60%;
  height:60%;
  border-radius: 50%;
  box-shadow: 0 0 10rpx #888;
}

.category_swiper {
  width: 100%;
  height: 250rpx;
}

.category_item .category_title {
  font-size: 20rpx;
  color: rgb(78, 77, 77);
}

.category_swiper .wx-swiper-dot {
  width: 28rpx;
  display: inline-flex;
  height: 4rpx;
  margin-left: 10rpx;
  justify-content: space-between;
}

.top_notice {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  background-color: white;
}

.notice_title {
  height: 60rpx;
  line-height: 60rpx;
  font-size: 30rpx;
  font-weight: 800;
  color: cadetblue;
  padding: 10rpx;
}

.swiper_notice {
  width: 87%;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: 100;
  font-size: 25rpx;
  letter-spacing: 2rpx;
}

.swiper_notice swiper-item {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.bottom_ad {
  margin-top: 10rpx;
}

.ad {
  height: 150rpx;
}

.ad image {
  width: 100%;
}

.recommend {
  height: 300rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
  margin-top: 10rpx;
}

.recommend_item_weather {
  background-color: white;
  width: 47%;
  display: flex;
  flex-direction: column;
  padding: 10rpx;
}

.recommend_title {
  font-weight: 800;
}

.recommend_item_weather .intro {
  display: flex;
  flex-direction: row;
}

.recommend_item_weather .intro_item {
  font-size: 25rpx;
}

.recommend_item_lost {
  position: relative;
  background-color: white;
  width: 47%;
  padding: 10rpx;
}

.recommend_item_lost .intro {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.recommend_item_lost .intro_item {
  margin-top: 10rpx;
  font-weight: 200;
}

.time_lost {
  position: absolute;
  bottom: 10rpx;
  font-size: 22rpx;
}

.lost_look {
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
  font-size: 25rpx;
  color: #999;
}

.intro_title_lost {
  word-wrap: break-word;
  font-size: 25rpx;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.recommend_item_service {
  background-color: white;
  width: 47%;
  display: flex;
  flex-direction: column;
  padding: 10rpx;
}

.recommend_item_service .intro_item {
  display: flex;
  flex-direction: row;
  text-align: center;
  font-weight: 300;
  font-size: 30rpx;
}

.recommend_item_service .intro_item view {
  border: 1rpx solid #999;
  margin: 10rpx;
  width: 50%;
  border-radius: 10rpx;
}

.recommend_item_eat {
  background-color: white;
  width: 47%;
  display: flex;
  flex-direction: column;
  padding: 10rpx;
}

scroll-view {
  height: 200rpx;
}

button::after {
  outline: none;
  border: none;
}

button {
  font-weight: 200;
  line-height: 60rpx;
  height: 60rpx;
  font-size: 25rpx;
  background-color: rgba(0, 0, 0, 0);
}

.eat_title {
  letter-spacing: 5rpx;
  margin-top: 10rpx;
  text-align: left;
  font-size: 30rpx;
  font-weight: 500;
}

.eat_phone {
  font-size: 25rpx;
  font-weight: 300;
  color: #999;
}

.title_category {
  margin-top: 10rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
}

.title_category view {
  text-align: center;
  width: 33%;
  padding: 10rpx;
  font-weight: 600;
  color: #999;
}

.main {
  margin-top: 10rpx;
  background-color: rgb(240, 240, 240);
}

.back {
  background-color: white;
}

.user_image image {
  width: 50rpx;
  border-radius: 50%;
}

.user_flag {
  display: flex;
  flex-direction: row;
}

.user_flag view {
  font-weight: 100;
  color: white;
  font-size: 20rpx;
  margin-right: 10rpx;
  padding: 5rpx;
  border-radius: 4rpx;
}

.flag_sex {
  display: flex;
  align-items: center;   
  justify-content: center;
  flex-direction: column;
  background-color: chocolate;
}

.flag_category {
  display: flex;
  align-items: center;   
  justify-content: center;
  flex-direction: column;
  background-color: teal;
}

.flag_major {
  display: flex;
  align-items: center;   
  justify-content: center;
  flex-direction: column;
  align-items:center;
  vertical-align:middle; 
  background-color: burlywood;
}

.flag_age {
  background-color: cornflowerblue;
}

.user_image {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.user_name {
  font-weight: 800;
  color: cadetblue;
}

.user_message {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 10rpx;
  border-bottom: 2rpx dashed #ccc;
}

.user_message image {
  width: 50rpx;
  border-radius: 50%;
}

.user_detail {
  display: flex;
  flex-direction: row;
  flex-grow: 2;
  justify-content: space-between;
  margin-left: 10rpx;
}

.main_middle {
  padding: 10rpx;
  text-align: center;
  display: flex;
  flex-direction: row;
}

.main_middle_intro {
  word-wrap: break-word;
  font-size: 30rpx;
  display: -webkit-box;
  text-align: left;
  overflow: hidden;
  -webkit-line-clamp: 6;
  -webkit-box-orient: vertical;
  letter-spacing: 5rpx;
}

.main_bottom {
  margin-bottom: 10rpx;
}

.main_middle_image {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: right;
  /* height: 80px; */
  width: 600px;
}

.main_middle_image image {
  width: 100rpx;
  height: 100rpx;
  margin: 5rpx;
  border-radius: 10rpx;
}
.change_zero{
  display: none;
}
.change_one  {
  display: inline-block;
  width: 120rpx;
  /* height: 50rpx; */
   margin: 5rpx;
  border-radius: 10rpx;
} 

.change_second {
  display: inline-block;
  /* height: 50rpx; */
  width: 240rpx;
  margin: 5rpx;
  border-radius: 10rpx;
}
.change_three{
  width: 400rpx;
}
.main_bottom_operation {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.goTop {
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
}
